<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="main.score.ScoreController"
            prefHeight="722.0"
            prefWidth="922.0">
    <children>
        <TableView
                fx:id="scoreBoard"
                layoutX="261.0" layoutY="32.0"
                prefHeight="664.0" prefWidth="627.0"
                AnchorPane.bottomAnchor="90.0"
                AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0">
                <columns>
                    <TableColumn fx:id="rank" sortable="false"/>
                    <TableColumn fx:id="usernames" text="Username"/>
                    <TableColumn fx:id="scores" text="Score"/>
                </columns>
        </TableView>
        <ImageView fx:id="backButton"
                   fitHeight="34.0"
                   fitWidth="170.0"
                   layoutX="376.0"
                   layoutY="646.0"
                   onMouseClicked="#backButtonOnMouseClicked"
                   onMouseEntered="#backButtonOnMouseEntered"
                   onMouseExited="#backButtonOnMouseExited"
                   AnchorPane.bottomAnchor="26.0"/>
    </children>
</AnchorPane>
